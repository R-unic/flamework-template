{
  "name": "FlameworkGame",
  "globIgnorePaths": [
    "**/package.json",
    "**/tsconfig.json"
  ],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "TS": {
        "$path": "dist/server"
      },
      "Tests": {
        "$path": "dist/tests"
      }
    },
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "rbxts_include": {
        "$path": "include",
        "node_modules": {
          "$className": "Folder",
          "@rbxts": {
            "$path": "node_modules/@rbxts"
          },
          "@flamework": {
            "$path": "node_modules/@flamework"
          }
        }
      },
      "TS": {
        "$path": "dist/shared"
      },
      "Scripts": {
        "$path": "dist/scripts"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "TS": {
          "$path": "dist/client"
        }
      }
    },
    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },
    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}